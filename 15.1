  
#include <iostream>
#include <cmath>
using namespace std;

int main()
{
    
int n, i, g;
cin >> n;
    
    int* a = new int [n];
   
    int* b = new int [n];
    
    for(i = 0; i < n; i++)
    {
        cin >> a[i];
    }
    cout << endl;
    for (i = 0; i < n; i++)
    {
        cin >> b[i];
    }
    
    for (i = 0; i < n; i++)
    {
        g = a[i];
        a[i] = b[i];
        b[i] = g;
    }
    
    for (i = 0; i < n; i++)
    {
        cout << b[i] << " ";
    }
    cout << endl;
    
    for (i = 0; i < n; i++)
    {
        cout << a[i] << " ";
    }
   
}
